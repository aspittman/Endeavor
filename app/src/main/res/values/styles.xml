<resources>

    <style name="AppTheme" parent="Theme.AppCompat.DayNight.DarkActionBar">

        <item name="colorPrimary">@color/PrimaryGreen</item>
        <item name="colorPrimaryDark">@color/PrimaryDarkGreen</item>
        <item name="colorAccent">@color/LightAccentGreen</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="SplashScreenTheme" parent = "Theme.AppCompat.NoActionBar">
    <item name="android:windowBackground">@mipmap/endeavor_header_foreground</item>
</style>
</resources>
